{
    "name": "crtest",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "check": "next lint && tsc --noEmit",
        "db:generate": "prisma migrate dev",
        "db:migrate": "prisma migrate deploy",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "lint:fix": "next lint --fix",
        "preview": "next build && next start",
        "typecheck": "tsc --noEmit",
        "start-ngrok": "ngrok http --url=pony-apt-filly.ngrok-free.app 3000",
        "start-debug-server": "dotenv -e .env.local next dev --turbopack",
        "start-debug-server-local": "dotenv -e .env.postgres next dev --turbopack",
        "dev": "pnpm run start-ngrok & pnpm run start-debug-server",
        "dev_local": "pnpm run start-ngrok & pnpm run start-debug-server-local",
        "dev_dbug": "NODE_OPTIONS='--inspect' next dev",
        "postinstall": "dotenv -e .env.local -- prisma generate",
        "lint": "next lint",
        "start": "next start",
        "ngrok": "ngrok http --url=pony-apt-filly.ngrok-free.app 3000"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^1.6.0",
        "@clerk/nextjs": "^6.31.1",
        "@prisma/client": "^6.14.0",
        "@t3-oss/env-nextjs": "^0.12.0",
        "@tanstack/react-query": "^5.85.3",
        "@trpc/client": "11.0.0-rc.561",
        "@trpc/react-query": "11.0.0-rc.561",
        "@trpc/server": "11.0.0-rc.561",
        "@vercel/speed-insights": "^1.2.0",
        "bcryptjs": "^2.4.3",
        "isomorphic-dompurify": "^2.26.0",
        "lightgallery": "^2.8.3",
        "next": "15.3.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "server-only": "^0.0.1",
        "superjson": "^2.2.2",
        "svix": "^1.73.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.33.0",
        "@tailwindcss/postcss": "^4.0.15",
        "@types/eslint": "^8.56.12",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/node": "^20.19.11",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "@typescript-eslint/eslint-plugin": "^8.39.1",
        "@typescript-eslint/parser": "^8.39.1",
        "babel-plugin-react-compiler": "19.1.0-rc.2",
        "dotenv-cli": "^8.0.0",
        "eslint": "^9.33.0",
        "eslint-config-next": "^15.4.6",
        "eslint-config-prettier": "^10.1.8",
        "mime": "^4.0.7",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "prisma": "^6.14.0",
        "react-env": "^0.0.0",
        "tailwindcss": "^4.0.15",
        "ts-toolbelt": "^9.6.0",
        "typescript": "5.9.2",
        "typescript-eslint": "^8.39.1"
    },
    "ct3aMetadata": {
        "initVersion": "7.39.3"
    },
    "packageManager": "pnpm@10.14.0",
    "pnpm": {
        "onlyBuiltDependencies": [
            "@clerk/shared",
            "@prisma/client",
            "@prisma/engines",
            "@vercel/speed-insights",
            "msgpackr-extract",
            "prisma",
            "sharp",
            "unrs-resolver"
        ]
    }
}